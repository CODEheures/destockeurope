{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravel/socialite": "^3.0",
        "caouecs/laravel-lang": "~3.0",
        "kalnoy/nestedset": "^4.2",
        "intervention/image": "^2.3",
        "spatie/flysystem-dropbox": "^1.0",
        "iatstuti/laravel-cascade-soft-deletes": "1.*",
        "paypal/rest-api-sdk-php": "*",
        "vinkla/vimeo": "^3.5",
        "pragmarx/firewall": "^1.0",
        "codeheures/laravel-utils": "^1.0",
        "mpdf/mpdf": "dev-master#4b5f88f7b826d3a67bff6a1d7bf43806dac33bb4",
        "sngrl/php-firebase-cloud-messaging": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-debugbar": "^2.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
